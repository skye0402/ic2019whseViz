{
	"_version": "1.12.0",
	"sap.app": {
		"id": "warehouseViewer.warehouseViewer",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"warehouseService": {
				"uri": "/srv_api/warehouse/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "warehouseViewer.warehouseViewer.view.whseView",
			"type": "XML",
			"async": true,
			"id": "whseView"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "warehouseViewer.warehouseViewer.i18n.i18n"
				}
			},
			"warehouseBins": {
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"operationMode": "Server",
					"synchronizationMode": "None",
					"groupId": "$auto",
					"autoExpandSelect": true
				},
				"dataSource": "warehouseService",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "warehouseViewer.warehouseViewer.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RoutewhseView",
				"pattern": "RoutewhseView",
				"target": ["TargetwhseView"]
			}],
			"targets": {
				"TargetwhseView": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "whseView",
					"viewName": "whseView"
				}
			}
		}
	}
}