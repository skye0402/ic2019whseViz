<mvc:View controllerName="warehouseViewer.warehouseViewer.controller.Grid"
		  xmlns="sap.suite.ui.microchart" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" xmlns:table="sap.ui.table">
	<m:Page title="{i18n>title}" showNavButton="true" navButtonPress="onNavBack" backgroundDesign="Transparent">
	<table:Table selectionMode="None" rows="{/Rows}">
		<table:columns>
			<table:Column>
				<m:Label text="Task ID"/>
				<table:template>
					<m:Label text="{Product ID}"/>
				</table:template>
			</table:Column>
			<table:Column>
				<m:Label text="Task Progress"/>
				<table:template>
					<RadialMicroChart size="Responsive" height="18px" percentage="{TargetDonePercentage}" valueColor="{TargetDoneColor}" press="press" total="{Target}"/>
				</table:template>
			</table:Column>
			<table:Column>
				<m:Label text="Period"/>
				<table:template>
					<m:Label text="{Period}"/>
				</table:template>
			</table:Column>
			<table:Column>
				<m:Label text="Unit Price"/>
				<table:template>
					<m:Label text="{Unit Price}"/>
				</table:template>
			</table:Column>
			<table:Column>
				<m:Label text="Responsible"/>
				<table:template>
					<m:Label text="{Sales Manager}"/>
				</table:template>
			</table:Column>
			<table:Column>
				<m:Label text="Supplier"/>
				<table:template>
					<m:Label text="{Supplier}"/>
				</table:template>
			</table:Column>
			<table:Column>
				<m:Label text="Supplier ID"/>
				<table:template>
					<m:Label text="{Supplier ID}"/>
				</table:template>
			</table:Column>
		</table:columns>
	</table:Table>
	</m:Page>
</mvc:View>
